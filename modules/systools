#!/usr/bin/env bash

for p in \
    htop lsof inotify-tools file nano tcpdump traceroute tree wol \
    the_silver_searcher psmisc iftop inetutils iptraf-ng mtr procps-ng arp-scan \
    iotop ranger ps_mem pydf zip unzip zsh zsh-completions iperf pdfgrep ethtool \
    rsync mlocate nethogs ngrep pv trash-cli sshfs strace unrar progress iputils \
    findutils tmux cryptsetup pciutils whois bind-tools wget curl aria2 grep \
    iproute2 iw ncdu
do
    do_install "$p"
done
